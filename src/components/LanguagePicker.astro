---
import { languages, ui } from "../i18n/ui";
import {} from "astro:i18n";
import { translateURL } from "../i18n/utils";
---

<ul>
  {
    Object.entries(languages).map(([lang, label]) => (
      <li>
        <a href={translateURL(Astro.url, lang as keyof typeof ui)}>{label}</a>
      </li>
    ))
  }
</ul>
